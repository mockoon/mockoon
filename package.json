{
  "name": "mockoon",
  "description": "Mockoon is the easiest and quickest way to run mock APIs locally. No remote deployment, no account required, open source.",
  "version": "1.11.0",
  "author": {
    "name": "Guillaume Monnet",
    "email": "hi@255kb.dev",
    "url": "https://255kb.dev/"
  },
  "license": "MIT",
  "repository": "https://github.com/mockoon/mockoon",
  "homepage": "https://mockoon.com",
  "bugs": {
    "url": "https://github.com/mockoon/mockoon/issues"
  },
  "browserslist": [
    "Chrome >= 80"
  ],
  "main": "electron-main.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .js,.ts .",
    "test": "xvfb-maybe ts-mocha --paths -p test/tsconfig.spec.json --timeout 20000 test/suites/**/*.spec.ts",
    "serve": "concurrently \"npm run serve:app\" \"wait-on -d 10000 ./dist && npm run serve:electron\"",
    "serve:app": "ng build --configuration=development --watch --output-hashing=all",
    "serve:electron": "electron ./dist --serve --remote-debugging-port=8888",
    "build:dev": "rm -rf ./dist && ng build --prod --configuration=development",
    "build:ci": "rm -rf ./dist && ng build --prod --configuration=ci",
    "build:prod": "rm -rf ./dist && ng build --prod --configuration=production",
    "clean-packages": "rm -rf ./packages",
    "package:win": "npm run clean-packages && electron-builder --publish never --win --config electron-builder.json",
    "package:linux": "npm run clean-packages && electron-builder --publish never --linux --config electron-builder.json",
    "package:mac": "npm run clean-packages && electron-builder --publish never --mac --config electron-builder.json"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 80,
    "trailingComma": "none"
  },
  "dependencies": {
    "@mockoon/commons": "1.0.1",
    "brace": "0.11.1",
    "electron-is-dev": "1.1.0",
    "electron-json-storage": "4.2.0",
    "electron-log": "4.1.0",
    "electron-window-state": "5.0.3",
    "mime-types": "2.1.26",
    "mkdirp": "1.0.3",
    "object-path": "0.11.5",
    "request": "2.88.2",
    "semver": "7.1.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "8.4.1",
    "@angular-builders/dev-server": "7.3.1",
    "@angular-devkit/build-angular": "0.803.28",
    "@angular-devkit/core": "8.3.25",
    "@angular-eslint/eslint-plugin": "0.6.0-beta.0",
    "@angular/animations": "8.2.14",
    "@angular/cli": "8.3.25",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/fire": "5.4.2",
    "@angular/forms": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@apidevtools/swagger-parser": "9.0.1",
    "@ng-bootstrap/ng-bootstrap": "5.3.0",
    "@types/chai": "4.2.11",
    "@types/chai-as-promised": "7.1.2",
    "@types/electron-json-storage": "4.0.0",
    "@types/lodash": "4.14.149",
    "@types/mime-types": "2.1.0",
    "@types/mkdirp": "1.0.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.31",
    "@types/object-path": "0.11.0",
    "@types/request": "2.48.4",
    "@types/semver": "7.1.0",
    "@types/uuid": "7.0.2",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "bootstrap": "4.4.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-exclude": "2.0.2",
    "codelyzer": "5.2.1",
